FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dictionaryReplacement
{
    boundary
    {
        bottom
        {
            type            wall;
            inGroups        1(wall);
        }

        "heater.*"
        {
            type            wall;
            inGroups        1(wall);
        }

        left
        {
            type            wall;
            inGroups        1(wall);
        }

        right
        {
            type            wall;
            inGroups        1(wall);
        }

        frontAndBack
        {
            type            empty;
            inGroups        1(empty);
        }
    }
}


