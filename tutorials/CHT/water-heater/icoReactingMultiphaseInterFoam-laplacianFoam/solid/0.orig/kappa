FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      kappa;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1  1 -3 -1 0 0 0];

internalField   uniform 16.2; // X5CrNi18-10 , 100 Â°C

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    "InterfaceHeater.*"
    {
        type            fixedValue;
        value           $internalField;
    }

    frontAndBack
    {
        type            empty;
    }
}

